   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "options = webdriver.ChromeOptions()\n",
    "service = ChromeService(executable_path=CHROMEDRIVER_PATH)\n",
    "driver = webdriver.Chrome(service=service, options=options)"
